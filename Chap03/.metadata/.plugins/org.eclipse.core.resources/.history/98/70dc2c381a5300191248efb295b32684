package Chap03_12;

public class Chap03_12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		int i = 0;

		try {
			do {
				sum += Integer.parseInt(args[i]);
			}while();
		} catch (NumberFormatException e) {

		}

		System.out.println(sum / args.length);
	}
}
