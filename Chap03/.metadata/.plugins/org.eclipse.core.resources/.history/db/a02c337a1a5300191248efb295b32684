package Chap03_12;

public class Chap03_12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		int i = 0;

		do {
			try {
				sum += Integer.parseInt(args[i]);
			} catch (NumberFormatException e) {

			} finally {
				i++;
			}
		} while (i < args.length);

		System.out.println(sum / args.length);
	}
}
