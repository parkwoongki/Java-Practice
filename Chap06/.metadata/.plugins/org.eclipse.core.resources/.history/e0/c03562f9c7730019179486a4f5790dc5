package Prob07;

import java.util.Scanner;

public class Prob07 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scanner = new Scanner(System.in);

		while (true) {
			System.out.print(">>");

			
			
			if(scanner.equals("그만"))
				break;
		}

		System.out.println("종료합니다.");
		
		scanner.close();
	}

}
